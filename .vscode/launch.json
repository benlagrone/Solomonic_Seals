    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Webserver",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/webserver.py",
            "console": "integratedTerminal",
            "env": {
                "PORT": "8080",
                "HOST": "0.0.0.0"
